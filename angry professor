c

#include<stdio.h>
int main()
{
    int t,i;
    scanf("%d",&t);
    for(i=1;i<=t;i++)
    {
        int a[1000],n,k,i,count=0;
        scanf("%d %d",&n,&k);
        for(i=0;i<n;i++)
        {
            scanf("%d ",&a[i]);
    
        }
        for(i=0;i<n;i++)
        {
            if(a[i] <= 0)
                count=count+1;
    
        }
        if(count < k)
            printf("YES\n");
        else
            printf("NO\n");
    }
    
    
return 0;
}
    
